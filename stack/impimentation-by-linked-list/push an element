void pushfront(struct node *head , int x)
{
  struct node * newnode =(struct node *)malloc(sizeof(stuct node))
  newnode->x=x;
  newnode->next=NULL;
  if(heah==null)
    head=newnode;
  else
  {
    newnode->next=head;
    head=newnode;
  }
}






